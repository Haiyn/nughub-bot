{
  "name": "nughub-bot",
  "version": "0.0.2",
  "description": "A Discord.js bot for The Nug Hub.",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node -r tsconfig-paths/register ./dist/index.js",
    "build": "rm -rf dist && tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "resolve": "tsc -p tsconfig.json --traceResolution",
    "watch": "npm run build && nodemon --watch \"src/**/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec npm run start",
    "test": "jest",
    "debug": "node --inspect node_modules/.bin/jest --runInBand",
    "lint": "eslint --fix . --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Haiyn/nughub-bot.git"
  },
  "author": "Haiyn",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Haiyn/nughub-bot/issues"
  },
  "homepage": "https://github.com/Haiyn/nughub-bot#readme",
  "dependencies": {
    "@discordjs/rest": "0.1.1-canary.0",
    "@types/ioredis": "4.27.8",
    "@types/node": "16.7.10",
    "discord-api-types": "0.24.0",
    "discord.js": "13.1.0",
    "dotenv": "10.0.0",
    "inversify": "5.1.1",
    "ioredis": "4.28.0",
    "mongoose": "6.0.4",
    "reflect-metadata": "0.1.13",
    "tslog": "3.2.0",
    "typescript": "4.3.5",
    "typescript-string-operations": "1.4.1"
  },
  "devDependencies": {
    "@jest/globals": "^27.3.1",
    "@types/jest": "27.0.2",
    "@typescript-eslint/eslint-plugin": "4.28.1",
    "@typescript-eslint/parser": "4.28.1",
    "eslint": "7.30.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-unused-imports": "1.1.4",
    "jest": "27.3.1",
    "jest-extended": "^1.1.0",
    "nodemon": "2.0.12",
    "prettier": "2.4.1",
    "prettier-plugin-organize-imports": "2.3.4",
    "ts-jest": "^27.0.7",
    "ts-node": "10.0.0",
    "tsc-alias": "1.3.9",
    "tsconfig-paths": "3.10.1",
    "tsconfig-paths-jest": "^0.0.1"
  }
}
