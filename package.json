{
  "name": "nughub-bot",
  "version": "0.0.1",
  "description": "A Discord.js bot for The Nug Hub.",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node -r tsconfig-paths/register ./dist/index.js",
    "build": "rm -rf dist && tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "resolve": "tsc -p tsconfig.json --traceResolution",
    "watch": "npm run build && nodemon --watch \"src/**/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec npm run start",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register \"tests/**/*.spec.ts\"",
    "lint": "eslint --fix . --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Haiyn/nughub-bot.git"
  },
  "author": "Haiyn",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Haiyn/nughub-bot/issues"
  },
  "homepage": "https://github.com/Haiyn/nughub-bot#readme",
  "dependencies": {
    "@types/mongoose": "^5.11.97",
    "@types/node": "^16.7.10",
    "discord.js": "^13.1.0",
    "dotenv": "^10.0.0",
    "inversify": "^5.1.1",
    "mongoose": "^6.0.4",
    "reflect-metadata": "^0.1.13",
    "tslog": "^3.2.0",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@types/chai": "^4.2.19",
    "@types/mocha": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "chai": "^4.3.4",
    "confinode": "^2.1.1",
    "eslint": "^7.30.0",
    "eslint-plugin-unused-imports": "^1.1.4",
    "mocha": "^9.0.1",
    "nodemon": "^2.0.12",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.0.0",
    "tsc-alias": "^1.3.9",
    "tsconfig-paths": "^3.10.1"
  }
}
