{
  "name": "nughub-bot",
  "version": "0.0.1",
  "description": "A Discord.js bot for The Nug Hub.",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node -r ./bootstrap-tsconfig-paths.js ./dist/index.js",
    "build": "rm -rf dist && tsc -p tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "resolve": "tsc -p tsconfig.json --traceResolution",
    "watch": "tsc -p tsconfig.json -w",
    "test": "mocha -r ts-node/register \"tests/**/*.spec.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Haiyn/nughub-bot.git"
  },
  "author": "Haiyn",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Haiyn/nughub-bot/issues"
  },
  "homepage": "https://github.com/Haiyn/nughub-bot#readme",
  "dependencies": {
    "@types/node": "^15.14.0",
    "discord.js": "^12.5.3",
    "dotenv": "^10.0.0",
    "inversify": "^5.1.1",
    "reflect-metadata": "^0.1.13",
    "tslog": "^3.2.0",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@types/chai": "^4.2.19",
    "@types/mocha": "^8.2.2",
    "chai": "^4.3.4",
    "mocha": "^9.0.1",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "tscpaths": "^0.0.9"
  }
}
